<springSet beingType="creature">
	
	<!-- 
		NOTES:  
		spring : The higher the spring value the 'tighter' the spring, causing the part to move towards it's leader with more force.
		damp : The higher the damp value the less dampening, a value of 1 would create no dampening.
	-->
	
	<springData>
		<part>head</part>
		<leader>neck</leader>
		<spring>0.5</spring>
		<damp>0.55</damp>
		<rotateByLeader>true</rotateByLeader>
		<offsetX>0</offsetX>
		<offsetY>0</offsetY>
	</springData>
	
	<springData>
		<part>hair</part>
		<leader>head</leader>
		<spring>0.4</spring>
		<damp>0.5</damp>
		<rotateByLeader>true</rotateByLeader>
		<offsetX>0</offsetX>
		<offsetY>0</offsetY>
	</springData>
	
</springSet>


