<bundle>
	<id>halloween2</id>
	<name>Halloween: Classic Monsters Pack</name>

	<cardSets>
		<cardSet id="shared"/>
		<cardSet id="male">
			<card>3077</card>
			<card>3073</card>
			<card>3138</card>
		</cardSet>
		<cardSet id="female">
			<card>3078</card>
			<card>3074</card>
			<card>3139</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>2</bundleNum>
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>0</brightness>
	   			<contrast>0</contrast>
	   			<saturation>-3</saturation>
	   			<hue>-4</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/pumpkin.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>