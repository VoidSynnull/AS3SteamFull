<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/TrollsWorldTourQuest/" ext="xml">
		<asset>MusicGame</asset>
		<asset>zipmanifest</asset>
	</data>

		<!-- interior xml files and swfs -->
