<events>
	<group type="triggered" id="startDwight">
		<event>
			<param1 id="action">lockInput</param1>
		</event>
		<event>
		<param1 id="action">moveToTarget</param1>
			<param2 id="npcID">dwight</param2>
			<param3 id="target">590,810</param3>
		</event>
		<event>
			<param1 id="action">wait</param1>
			<param2 id="delay">2</param2>
		</event>
		<event>
			<param1 id="action">talk</param1>
			<param2 id="npcID">dwight</param2>
			<param3 id="sayID">startDialog</param3>
		</event>
	</group>
	<group type="triggered" id="rafeTalk">
	<event>
			<param1 id="action">talk</param1>
			<param2 id="npcID">rafe</param2>
			<param3 id="sayID">runDialog</param3>
		</event>
	</group>
	
	<group type="triggered" id="startGame">
	<event>
			<param1 id="action">callFunction</param1>
			<param2 id="function">loadStartPopup</param2>
	</event>
	</group>
</events>