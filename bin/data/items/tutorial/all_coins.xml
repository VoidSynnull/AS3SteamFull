<item id="all_coins">
	<type>island</type>
	<subType>tutorial</subType>
	
	<contentClass>
		<className>game.ui.card.MultiFrameContentView</className>
		<parameters>
			<param>items/tutorial/all_coins.swf</param>
		</parameters>
	</contentClass>
	
	<textfields>
 		<text id="title">
      		<value>Tutorial Coins</value>
