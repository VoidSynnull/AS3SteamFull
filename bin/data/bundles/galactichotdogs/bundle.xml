			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>0</brightness>
	   			<contrast>0</contrast>
	   			<saturation>0</saturation>
	   			<hue>15</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/galactichotdogs.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>