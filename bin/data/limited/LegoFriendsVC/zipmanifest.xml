<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/LegoFriendsVC/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset> <!-- remove this if no new code added for this campaign -->
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/LegoFriendsVC/">
		<asset>custom.xml</asset>
		<asset>dialog.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>tracking.xml</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/LegoFriendsVC/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
		<asset>LegoFriend.swf</asset>
	</assets>
	
	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2872</asset>
	</data>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundBlue</asset>
	</assets>

	<!-- avatar parts -->
	<data type="overshirt" path="data/entity/character/parts/overshirt/" prefix="limited_"  ext="xml">
		<asset>legokart</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" prefix="limited_"  ext="swf">
		<asset>legokart</asset>
	</assets>
</manifest>