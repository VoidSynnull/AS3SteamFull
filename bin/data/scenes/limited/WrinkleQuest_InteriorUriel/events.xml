<events>
	<group type="triggered" id="initScene">
		<event>
			<param1 id="action">talk</param1>
			<param2 id="npcID">Meg</param2>
			<param3 id="sayID">init</param3>
		</event>
	</group>
	<group type="triggered" id="travel">
		<event>
			<param1 id="action">lockInput</param1>
		</event>
		<event>
			<param1 id="action">setPart</param1>
			<param2 id="partID">item</param2>
			<param3 id="partValue">empty</param3>
			<param3 id="charID">MrsWhatsit</param3>
		</event>
		<event>
			<param1 id="action">setPart</param1>
			<param2 id="partID">facial</param2>
			<param3 id="partValue">limited_wrinkle_orb_glasses</param3>
		</event>
		<event>
			<param1 id="action">wait</param1>
			<param2 id="delay">1</param2>
		</event>
		<event>
			<param1 id="action">showOverlayAnim</param1>
			<param2 id="path">limited/WrinkleQuest/tesser.swf</param2>
			<param3 id="scaleMode">scaleToFill</param3>
		</event>
		<event>
			<param1 id="action">callFunction</param1>
			<param2 id="function">loadScene</param2>
			<param3 id="target">WrinkleQuest_InteriorCamazotz</param3>
			<param4 id="noFades">true</param4>
		</event>
	</group>	
</events>