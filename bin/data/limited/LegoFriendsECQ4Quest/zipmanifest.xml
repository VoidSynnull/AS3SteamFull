<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/LegoFriendsECQ4Quest/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
		<asset>version</asset> <!-- remove this if no new code added for this campaign -->
	</data>

	<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/LegoFriendsECQ4Quest_Interior/">
		<asset>dialog.xml</asset>
		<asset>doors.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>sounds.xml</asset>
		<asset>tracking.xml</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/LegoFriendsECQ4Quest_Interior/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/LegoFriendsECQ4Quest/" ext="swf">
		<asset>StartSynopsis1</asset>
		<asset>StartSynopsis2</asset>
		<asset>cameraAnim1</asset>
		<asset>cameraAnim2</asset>
		<asset>cameraAnim3</asset>
		<asset>cameraAnim4</asset>
		<asset>SummerGirl1</asset>
		<asset>SummerGirl2</asset>
		<asset>WinterGirl1</asset>
		<asset>surfingseal</asset>
	</assets>
	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2865</asset>
		<asset>2866</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2865</asset>
		<asset>2866</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundBlue</asset>
	</assets>

	<!-- animation xml files -->
	<data type="animation" path="data/entity/character/animation/human/" ext="xml">
		<asset>salute</asset>
	</data>

	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
	</assets>

	<!-- avatar parts -->
	<data type="item" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
		<asset>pink_phone</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" prefix="limited_" ext="swf">
		<asset>pink_phone</asset>
	</assets>
</manifest>