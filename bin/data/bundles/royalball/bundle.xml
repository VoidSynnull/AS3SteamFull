<bundle>
	<id>royalball</id>
	<name>Royal Ball Pack</name>
	
	<cardSets>
		<cardSet id="female">
			<card>3403</card>
			<card>3404</card>
			<card>3406</card>
		</cardSet>
		<cardSet id="male">
			<card>3405</card>
			<card>3407</card>
			<card>3408</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>0</bundleNum>
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>0</brightness>
	   			<contrast>0</contrast>
	   			<saturation>12</saturation>
	   			<hue>156</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/royalball.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>