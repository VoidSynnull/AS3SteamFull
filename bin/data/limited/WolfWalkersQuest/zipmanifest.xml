<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/WolfWalkersQuest/" ext="xml">
		<asset>zipmanifest</asset>
	</data>

		<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/WolfWalkersQuest_Interior/" ext="xml">
		<asset>dialog</asset>
		<asset>doors</asset>
		<asset>events</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/WolfWalkersQuest_Interior/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
		<asset>foreground</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/WolfWalkersQuest_Game/" ext="xml">
		<asset>game</asset>
		<asset>hits</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/WolfWalkersQuest_Game/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
		<asset>foreground</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/WolfWalkersQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>npc</asset>
		<asset>wolf_power</asset>
	</assets>
	
	<data type="ability" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_wolf_walker</asset>
	</data>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2043</asset>
		<asset>2044</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2043</asset>
		<asset>2044</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundOrange</asset>
		<asset>AdBackgroundBlue</asset>
	</assets>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/limited/Wolfwalkers/" ext="mp3" keep="true">
		<asset>ForestThemes</asset>
		<asset>Mechanical</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>ls_dirt_01</asset>
		<asset>ls_dirt_02</asset>
		<asset>ls_wood_01</asset>
		<asset>ls_wood_02</asset>
		<asset>ls_grass_01</asset>
		<asset>ls_grass_02</asset>
		<asset>ls_concrete_01</asset>
		<asset>ls_concrete_02</asset>
		<asset>ls_bounce_01</asset>
		<asset>ls_bounce_02</asset>
		<asset>ls_bounce_03</asset>
		<asset>fs_dirt_01</asset>
		<asset>fs_dirt_02</asset>
		<asset>fs_dirt_03</asset>
		<asset>fs_dirt_04</asset>
		<asset>fs_wood_01</asset>
		<asset>fs_wood_02</asset>
		<asset>fs_wood_03</asset>
		<asset>fs_wood_04</asset>
		<asset>fs_grass_01</asset>
		<asset>fs_grass_02</asset>
		<asset>fs_grass_03</asset>
		<asset>fs_grass_04</asset>
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
	</assets>

	<!-- avatar parts -->
	<data type="item" path="data/entity/character/parts/item/" ext="xml">
		<asset>limited_dora_torch_npc</asset>
		<asset>limited_wolf_power_crossbow</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" ext="swf">
		<asset>limited_dora_torch_npc</asset>
		<asset>limited_wolf_power_crossbow</asset>
	</assets>
	<data type="pack" path="data/entity/character/parts/pack/" ext="xml">
		<asset>limited_mebhe_wolf_tail</asset>
		<asset>limited_crossbow_cape</asset>
	</data>
	<assets type="pack" path="assets/entity/character/pack/" ext="swf">
