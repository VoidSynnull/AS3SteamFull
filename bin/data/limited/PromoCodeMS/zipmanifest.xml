<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/PromoCodeMS/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
		<asset>classes</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/PromoCodeMS/mainStreet/">
		<asset>scene.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>tracking.xml</asset>
		<asset>custom.xml</asset>
	</data>
	
	<assets type="support" path="assets/limited/PromoCodeMS/" ext="swf">
		<asset>WhoopeeCushion</asset>
	</assets>
	
	<data type="support" path="data/entity/character/animation/human/" ext="xml">
		<asset>place</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/PromoCodeMS/mainStreet/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
	</assets>
	
			<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2697</asset>
	</data>
	
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2697</asset>
	</assets>
	
			<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundGreen</asset>
	</assets>
	
			<data type="hair" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
		<asset>whoopee</asset>
	</data>
	<assets type="hair" path="assets/entity/character/item/" prefix="limited_" ext="swf">
		<asset>whoopee</asset>
	</assets>
	
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fart_01</asset>
		<asset>fart_02</asset>
		<asset>fart_03</asset>
		<asset>fart_04</asset>
	</assets>
	
	
</manifest>