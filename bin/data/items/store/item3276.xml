<item id="3276">
	<type>store</type>
	
	<contentClass>
		<className>game.ui.card.MovieClipContentView</className>
		<parameters>
			<param>specialAbility/objects/statues/plantStatueCard1.swf</param>
			<param>specialAbility/objects/statues/plantStatueCard2.swf</param>
			<param>specialAbility/objects/statues/plantStatueCard3.swf</param>
		</parameters>
	</contentClass>
	
	<buttons>
		<btn>
			<conditional id="hasLook" isTrue="false"/>
			<label>ACTIVATE</label>
			<actions>
				<action type="applyLook"/>
			</actions>
		</btn>
		<btn>
			<conditional id="hasLook" isTrue="true"/>
			<label>TURN OFF</label>
			<actions>
				<action type="removeLook"/>
			</actions>
		</btn>
	</buttons>
	
	<radiobuttons>
		<btn>
			<color>0x8ACA8D</color>
			<val>0</val>
		</btn>
		<btn>
			<color>0x68A666</color>
			<val>1</val>
		</btn>
		<btn>
			<color>0x558C59</color>
			<val>2</val>
		</btn>
	</radiobuttons>
	
	<textfields>
		<text id="storeSubTitle">
			<value>Poptropolis 2013</value>
		</text>
		<text id="storeTitle">
			<value>Plant-A-Statue</value>
		</text>
	</textfields>
	
	<assets>
 		<asset id="cardBack" />
	</assets>
	
	<looks>
		<look>
			<item>storeplantastatue1</item>
		</look>
		<look>
			<item>storeplantastatue2</item>
		</look>
		<look>
			<item>storeplantastatue3</item>
		</look>
	</looks>
	
	<conditionals>
		<conditional type="hasLook"/>
	</conditionals>
</item>