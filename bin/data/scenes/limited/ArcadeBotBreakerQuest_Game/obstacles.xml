		<item name="tire2" />
	</obstacle>
	<obstacle baseName="fire" type="static"  x="60" spacingY="2000" hitSpeed="-300" hitTime="2" >
		<item name="fire1" />
	</obstacle>
	<obstacle baseName="finish" type="placement" >
		<item name="finish1" />
	</obstacle>

