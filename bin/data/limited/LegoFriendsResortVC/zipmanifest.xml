<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/LegoFriendsResortVC/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/LegoFriendsResortVC/" ext="xml">
		<asset>custom</asset>
		<asset>dialog</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/LegoFriendsResortVC/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>
	
	<!-- bitmap npcs -->
	<assets type="game" path="assets/limited/LegoFriendsQuest/" ext="swf">
		<asset>mia</asset>
		<asset>cameraAnim1</asset>
		<asset>cameraAnim2</asset>
		<asset>cameraAnim3</asset>
		<asset>cameraAnim4</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2929</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2929</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundBlue</asset>
	</assets>
	<!-- avatar parts -->
	<data type="item" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
		<asset>legofriends_walkie_talkie</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" prefix="limited_" ext="swf">
		<asset>legofriends_walkie_talkie</asset>
	</assets>
</manifest>