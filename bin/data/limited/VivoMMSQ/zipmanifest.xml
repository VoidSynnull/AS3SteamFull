<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/VivoMMSQ/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/VivoMMSQ/" ext="xml">
		<asset>dialog</asset>
		<asset>doors</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/VivoMMSQ/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>
	<!-- bitmap npc from quest folder -->
	<assets type="game" path="assets/limited/VivoQuest/" ext="swf">
		<asset>Gabi</asset>
		<asset>Vivo</asset>
	</assets>
</manifest>