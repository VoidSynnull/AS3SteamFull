<item id="3376">
	<type>store</type>
	<subType>general</subType>
	<campaignID>PrintPoster</campaignID>
	<buttons>
		<btn>
		<label>PRINT</label>
		<actions>
			<action type="printPoster" tracking="Use Shrink Ray Poster Card"> <!-- change tracking call -->
				<parameters>
					<param id="path">posters/ShrinkRay.swf</param>
					<param id="xpos">1230</param>
					<param id="ypos">1630</param>
					<param id="xscale">5.5</param>
					<param id="yscale">5.5</param>
					<param id="rotation">20</param>
					<param id="pose">Tremble</param>
					<param id="frame">6</param>
