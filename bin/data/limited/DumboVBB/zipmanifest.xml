<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/DumboVBB/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/DumboVBB/" ext="xml">
		<asset>custom</asset>
		<asset>dialog</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/DumboVBB/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
		<asset>dumbo</asset>
	</assets>
	<assets type="game" path="assets/limited/DumboVBB/" ext="mp3">
		<asset>dumbo</asset>
	</assets>
	
	<!-- followers -->
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>follower_dumbo</asset>
	</data>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2962</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2962</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundYellow</asset>
	</assets>

	<!-- avatar parts -->
	<data type="mouth" path="data/entity/character/parts/mouth/" keep="true" ext="xml">
		<asset>sponsorec2</asset>
		<asset>skullchinamerch</asset>
	</data>
	<assets type="mouth" path="assets/entity/character/mouth/" keep="true" ext="swf">
		<asset>sponsorec2</asset>
		<asset>skullchinamerch</asset>
	</assets>
	<data type="marks" path="data/entity/character/parts/marks/" keep="true" ext="xml">
		<asset>snootylady</asset>
	</data>
	<assets type="marks" path="assets/entity/character/marks/" keep="true" ext="swf">
		<asset>snootylady</asset>
	</assets>
	<data type="hair" path="data/entity/character/parts/hair/" prefix="limited_" ext="xml">
		<asset>ringleader</asset>
		<asset>acrobat</asset>
	</data>
	<assets type="hair" path="assets/entity/character/hair/" prefix="limited_" ext="swf">
		<asset>ringleader</asset>
		<asset>acrobat</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" prefix="limited_" ext="xml">
		<asset>ringleader</asset>
	</data>
	<assets type="pants" path="assets/entity/character/pants/" prefix="limited_" ext="swf">
		<asset>ringleader</asset>
	</assets>
	<data type="pack" path="data/entity/character/parts/pack/" prefix="limited_" ext="xml">
		<asset>ringleader</asset>
	</data>
	<assets type="pack" path="assets/entity/character/pack/" prefix="limited_" ext="swf">
		<asset>ringleader</asset>
	</assets>
	<data type="item" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
		<asset>magic_feather</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" prefix="limited_" ext="swf">
		<asset>magic_feather</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" prefix="limited_" ext="xml">
		<asset>ringleader</asset>
		<asset>acrobat</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" prefix="limited_" ext="swf">
		<asset>ringleader</asset>
		<asset>acrobat</asset>
	</assets>
</manifest>