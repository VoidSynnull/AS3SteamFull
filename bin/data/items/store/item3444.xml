<item id="3444">
	<type>store</type>
	<subType>general</subType>
	<campaignID>PrintPoster</campaignID>
	<buttons>
		<btn>
		<label>PRINT</label>
		<actions>
			<action type="printPoster" tracking="Use Timmy Failure Poster Card"> <!-- change tracking call -->
				<parameters>
					<param id="path">posters/TimmyPoster.swf</param>
					<param id="xpos">450</param>
					<param id="ypos">895</param>
					<param id="xscale">-1.9</param>
					<param id="yscale">1.9</param>
					<param id="rotation">-15</param>
