<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/MagiswordsQuest/" ext="xml">
		<asset>campaign</asset>
		<asset>zipmanifest</asset>
	</data>
	<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/MagiswordsQuest_Interior/">
		<asset>scene.xml</asset>
		<asset>dialog.xml</asset>
		<asset>npcs.xml</asset>
		<asset>hits.xml</asset>
		<asset>doors.xml</asset>
		<asset>sounds.xml</asset>
		<asset>tracking.xml</asset>
		<asset>events.xml</asset>
		<asset>hotspots.xml</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/MagiswordsQuest_Interior/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
		<asset>foreground.swf</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/MagiswordsQuest_Game/">
		<asset>scene.xml</asset>
		<asset>hits.xml</asset>
		<asset>npcs.xml</asset>
		<asset>game.xml</asset>
		<asset>sounds.xml</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/MagiswordsQuest_Game/">
		<asset>background.swf</asset>
		<asset>foreground.swf</asset>
		<asset>interactive.swf</asset>
		<asset>backdrop.swf</asset>
	</assets>
	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/MagiswordsQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>Vambre_NPC</asset>
		<asset>Prohyas_NPC</asset>
		<asset>tomato</asset>
	</assets>
	
		<!-- animation xml files -->
	<data type="animation" path="data/entity/character/animation/human/" ext="xml" keep="true">
		<asset>salute</asset>
		<asset>pointItem</asset>
	</data>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>ad_interior_scene</asset>
		<asset>collection_game</asset>
	</assets>
	
		<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2813</asset>
		<asset>2814</asset>
	</data>
	
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2813</asset>
		<asset>2814</asset>
	</assets>

	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundBrightGreen</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
		<asset>ls_concrete_01</asset>
		<asset>ls_concrete_02</asset>
		<asset>ls_bounce_01</asset>
		<asset>ls_bounce_02</asset>
		<asset>ls_bounce_03</asset>
		<asset>explosion_01</asset>
		<asset>points_ping_02e</asset>
	</assets>

			 
	
	<data type="item" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
	<asset>magiswords_oinkus</asset>
	<asset>magiswords_dolphin</asset>
	<asset>tomatosword</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" prefix="limited_" ext="swf">
	<asset>magiswords_oinkus</asset>
	<asset>magiswords_dolphin</asset>
	<asset>tomatosword</asset>
	</assets>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>tomatomess</asset>
	</assets>
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>tomatomess</asset>
	</data>
	
	

</manifest>