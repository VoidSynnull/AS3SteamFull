<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/AntManAndTheWaspMVU/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/AntManAndTheWaspMVU/" ext="xml">
		<asset>custom</asset>
		<asset>dialog</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>tracking</asset>
		<asset>Fly</asset>
		<asset>motionMaster</asset>
		<asset>segmentPatterns</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/AntManAndTheWaspMVU/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>
	
	<!-- bitmap npcs -->
	<assets type="game" path="assets/limited/AntManAndTheWaspMVU/" ext="swf">
		<asset>ChooseFly</asset>
		<asset>LoseFly</asset>
		<asset>StartFly</asset>
		<asset>WinFly</asset>
		<asset>Fly</asset>
	</assets>
	
	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>mfb_battle_variation</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>flesh_impact_01</asset>
		<asset>flesh_impact_02</asset>
		<asset>glass_break_02</asset>
		<asset>can_impact_01</asset>
		<asset>metal_impact_12</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2902</asset>
		<asset>2906</asset>
	</data>
	
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundOrange</asset>
	</assets>

	<!-- animation xml files -->
	<data type="animation" path="data/entity/character/animation/human/" ext="xml">
		<asset>limited/waspFly</asset>
		<asset>standNinja</asset>
	</data>

	<!-- avatar parts -->
	<data type="mouth" path="data/entity/character/parts/mouth/" ext="xml">
		<asset>p_wafflegirl</asset>
	</data>
	<assets type="mouth" path="assets/entity/character/mouth/" ext="swf">
		<asset>p_wafflegirl</asset>
	</assets>
	<data type="facial" path="data/entity/character/parts/facial/" ext="xml">
		<asset>limited_ant_man</asset>
		<asset>limited_wasp</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" ext="swf">
		<asset>limited_ant_man</asset>
		<asset>limited_wasp</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" ext="xml">
		<asset>limited_ant_man_suit</asset>
		<asset>limited_wasp_suit</asset>
	</data>
	<assets type="pants" path="assets/entity/character/pants/" ext="swf">
		<asset>limited_ant_man_suit</asset>
		<asset>limited_wasp_suit</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" ext="xml">
		<asset>limited_ant_man_suit</asset>
		<asset>limited_wasp_suit</asset>
		<asset>limited_ant_man</asset>
		<asset>limited_wasp</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" ext="swf">
		<asset>limited_ant_man_suit</asset>
		<asset>limited_wasp_suit</asset>
		<asset>limited_ant_man</asset>
		<asset>limited_wasp</asset>
	</assets>
</manifest>