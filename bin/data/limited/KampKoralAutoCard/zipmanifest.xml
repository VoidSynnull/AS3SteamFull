	</data>

	<assets type="sounds" path="sound/limited/SpongeBobMovie/" ext="mp3" keep="true">
		<asset>spongeLaugh</asset>
	</assets>

	<!-- animation xml files -->
	<data type="ability" path="data/entity/character/animation/human/" ext="xml">
		<asset>salute</asset>
