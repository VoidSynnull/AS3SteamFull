<item id="2017"> <!-- Change this card ID -->
	<type>custom</type>
	<subType>general</subType>
	<name>American Girl Bot Breaker</name> <!-- *** Change this card name -->
  	<campaignID>AmericanGirlQuest</campaignID> <!-- *** Change this CMS name -->
	
		
		<buttons>
			<btn>
				<index>1</index>
				<label>PLAY</label>
				<actions>
				<action type="loadScene" tracking="true">
					<parameters>
						<param id="scene">game.scenes.hub.arcadeGame.ArcadeGame</param>
						<param id="arcadeGame">BotBreaker</param>
						<param id="overrideReturnScene">true</param>
					</parameters>
				</action>
			</actions>
			</btn>
			<btn>
				<index>0</index>
				<label>VISIT WEBSITE</label>
				<actions>
					<action type="gotoUrl" tracking="true" />
				</actions>
			</btn>
		</buttons>
	
		<textfields>
		<text id="limited">
				<value>FOR A LIMITED TIME ONLY</value>
			</text>
			<text id="subtitle">
				<value>American Girl</value> <!-- Change this subtitle -->
			</text>
			<text id="adtitle">
				<value>Bot Breaker</value> <!-- Change this title -->
			</text>
			<text id="ad">
				<value>ADVERTISEMENT</value>
			</text>
			<text id="memberinstructions">
				<web>Get a high score!</web>
				<mobile>Get a high score!</mobile>
				<y>60</y>
			</text>
		</textfields>
	
		<assets>
		<asset id="cardBack">
				<assetPath>items/limited/backs/AdBackgroundPink.swf</assetPath>
			</asset>
			<asset id="cardContent" />
		</assets>
		
</item>
