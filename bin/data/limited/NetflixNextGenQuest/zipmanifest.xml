<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/NetflixNextGenQuest/" ext="xml">
		<asset>zipmanifest</asset>
	</data>

	<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/NetflixNextGenQuest_Interior/" ext="xml">
		<asset>dialog</asset>
		<asset>doors</asset>
		<asset>events</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/NetflixNextGenQuest_Interior/" ext="swf">
		<asset>backdrop</asset>
		<asset>background</asset>
		<asset>background1</asset>
		<asset>background2</asset>
		<asset>interactive</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/NetflixNextGenQuest_Game/" ext="xml">
		<asset>game</asset>
		<asset>hits</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/NetflixNextGenQuest_Game/" ext="swf">
		<asset>background1</asset>
		<asset>background2</asset>
		<asset>interactive</asset>
		<asset>foreground1</asset>
		<asset>foreground2</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/NetflixNextGenQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>7723Follower</asset>
		<asset>7723_NPC</asset>
		<asset>ray</asset>
		<asset>mai_NPC</asset>
		<asset>toothbrush_NPC</asset>
		<asset>hairbrush_NPC</asset>
		<asset>molly_NPC</asset>
		<asset>momo</asset>
		<asset>pin_NPC</asset>
		<asset>ramen_NPC</asset>
		<asset>rice</asset>
		<asset>bob</asset>
	</assets>
	
	<assets type="support" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>follower_7723</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2914</asset>
		<asset>2915</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2914</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundPurple</asset>
	</assets>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>epic_boss_battle</asset>
		<asset>space_quest</asset>
	</assets>

	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>ls_bounce_01</asset>
		<asset>ls_bounce_02</asset>
		<asset>ls_bounce_03</asset>
		<asset>cart_bump_01</asset>
		<asset>fs_concrete_04</asset>
		<asset>explosion_01</asset>
		<asset>water_splash_06</asset>
		<asset>machine_impact_02</asset>
		<asset>turn_engine_on_01</asset>
	</assets>

	<!-- avatar parts -->
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>mai</asset>
		<asset>nextgen_7723</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>mai</asset>
		<asset>nextgen_7723</asset>
	</assets>
	<data type="hair" path="data/entity/character/parts/hair/" prefix="limited_" ext="xml">
		<asset>mai</asset>
	</data>
	<assets type="hair" path="assets/entity/character/hair/" prefix="limited_" ext="swf">
		<asset>mai</asset>
	</assets>
		<data type="pack" path="data/entity/character/parts/pack/" prefix="limited_" ext="xml">
		<asset>mai</asset>
	</data>
	<assets type="pack" path="assets/entity/character/pack/" prefix="limited_" ext="swf">
		<asset>mai</asset>
	</assets>
	<data type="shirt" path="data/entity/character/parts/shirt/" prefix="limited_" ext="xml">
		<asset>mai</asset>
	</data>
	<assets type="shirt" path="assets/entity/character/shirt/" prefix="limited_" ext="swf">
		<asset>mai</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" prefix="limited_" ext="xml">
		<asset>mai</asset>
	</data>
	<assets type="pants" path="assets/entity/character/pants/" prefix="limited_" ext="swf">
		<asset>mai</asset>
	</assets>
</manifest>