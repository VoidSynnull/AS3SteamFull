<item id="2058"> <!-- Change this card ID -->
  	<type>custom</type>
	<subType>general</subType>
	<name>Disney Bake Off Sprinkle Cannon</name> <!-- *** Change this card name -->
  	<campaignID>DisneyBakeOffQuest</campaignID> <!-- *** Change this CMS name -->
	
  	<buttons>
  		<btn>
  			<index>1</index>
 			<conditional id="hasLook" isTrue="true"/>
    		<label>PUT AWAY</label>
    		<actions>
				<action type="removeLook"/>
			</actions>
  		</btn>
  		<btn>
  			<index>1</index>
			<conditional id="hasLook" isTrue="false"/>
    		<label>USE</label>
    		<actions>
				<action type="applyLook" tracking="true" />
			</actions>
  		</btn>
		<btn>
    		<index>0</index>
				<label>VISIT WEBSITE</label>
				<actions>
					<action type="gotoUrl" tracking="true" />
	  		</actions>
    	</btn>
  	</buttons>
	
  	<textfields>
		<text id="limited">
			<value>FOR A LIMITED TIME ONLY</value>
		</text>
		<text id="subtitle">
			<value>Disney Magic Bake-Off</value> <!-- Change this subtitle -->
		</text>
		<text id="adtitle">
			<value>Sprinkles Cannon</value> <!-- Change this title -->
		</text>
		<text id="ad">
			<value>ADVERTISEMENT</value>
		</text>
		<text id="memberinstructions"> <!-- Change any member instructions -->
			<web>Press space bar to shoot</web>
			<mobile>Press action button to shoot</mobile>
			<y>66</y>
		</text>
  	</textfields>
	
  	<assets>
		<asset id="cardBack">
			<assetPath>items/limited/backs/AdBackgroundPink.swf</assetPath> <!-- Change this background zip -->
		</asset>
    	<asset id="cardContent" />
  	</assets>
  	
	<conditionals>
    	<conditional type="hasLook" />
    </conditionals>
    
  	<looks>
	    <look>
			<item>limited_sprinkles_canon</item>
		</look>
	</looks> 
</item>
