<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/CocoQuest/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
		<asset>version</asset> <!-- remove this if no new code added for this campaign -->
	</data>

		<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/CocoQuest_Interior/">
		<asset>dialog.xml</asset>
		<asset>doors.xml</asset>
		<asset>events.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>sounds.xml</asset>
		<asset>tracking.xml</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/CocoQuest_Interior/">
		<asset>backdrop.swf</asset>
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
		<asset>foreground.swf</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/CocoQuest_Game/">
		<asset>game.xml</asset>
		<asset>hits.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>sounds.xml</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/CocoQuest_Game/">
		<asset>background1.swf</asset>
		<asset>background2.swf</asset>
		<asset>backdrop1.swf</asset>
		<asset>backdrop2.swf</asset>
		<asset>interactive.swf</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/CocoQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>miguel</asset>
		<asset>hector</asset>
		<asset>delacruz</asset>
		<asset>petal</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2859</asset>
		<asset>2860</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2859</asset>
		<asset>2860</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundOrange</asset>
	</assets>

	<!-- animation xml files -->
	<data type="animation" path="data/entity/character/animation/human/" ext="xml">
		<asset>guitarStrum</asset>
	</data>
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_dante</asset>
	</data>

	<!-- sound assets -->
	<assets type="sounds" path="sound/music/" ext="mp3">
		<asset>coco_chasemusic</asset>
		<asset>coco_interiormusic</asset>
	</assets>
	<assets type="sounds" path="sound/limited/CocoQuest/" ext="mp3">
		<asset>coco_guitar</asset>
	</assets>
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
	</assets>

	<!-- avatar parts -->
	<data type="item" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
		<asset>guitar_coco</asset>
		<asset>legbone</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" prefix="limited_" ext="swf">
		<asset>guitar_coco</asset>
		<asset>legbone</asset>
	</assets>
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>dante</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>dante</asset>
	</assets>
	<data type="pack" path="data/entity/character/parts/pack/" prefix="limited_" ext="xml">
		<asset>dante</asset>
	</data>
	<assets type="pack" path="assets/entity/character/pack/" prefix="limited_" ext="swf">
		<asset>dante</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" prefix="limited_" ext="xml">
		<asset>dante</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" prefix="limited_" ext="swf">
		<asset>dante</asset>
	</assets>
</manifest>