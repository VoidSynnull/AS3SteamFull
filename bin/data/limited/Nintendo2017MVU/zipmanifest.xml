<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/Nintendo2017MVU/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/Nintendo2017MVU/mainStreet/">
		<asset>custom.xml</asset>
		<asset>dialog.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>tracking.xml</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/Nintendo2017MVU/mainStreet/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
	</assets>
	
	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2849</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2849</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundPink</asset>
	</assets>

	<!-- avatar parts -->
	<data type="mouth" path="data/entity/character/parts/mouth/" ext="xml" keep="true">
		<asset>14</asset>
	</data>
	<assets type="mouth" path="assets/entity/character/mouth/" ext="swf" keep="true">
		<asset>14</asset>
	</assets>
	<data type="marks" path="data/entity/character/parts/marks/" ext="xml" keep="true">
		<asset>lc_astronaut</asset>
	</data>
	<assets type="marks" path="assets/entity/character/marks/" ext="swf" keep="true">
		<asset>lc_astronaut</asset>
	</assets>
	<data type="facial" path="data/entity/character/parts/facial/" ext="xml" keep="true">
		<asset>lc_astronaut</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" ext="swf" keep="true">
		<asset>lc_astronaut</asset>
	</assets>
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>rasberry</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>rasberry</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" ext="xml" keep="true">
		<asset>directord</asset>
	</data>
	<assets type="pants" path="assets/entity/character/pants/" ext="swf" keep="true">
		<asset>directord</asset>
	</assets>
	<data type="pack" path="data/entity/character/parts/pack/" ext="xml" keep="true">
		<asset>lc_astronaut</asset>
	</data>
	<assets type="pack" path="assets/entity/character/pack/" ext="swf" keep="true">
		<asset>lc_astronaut</asset>
	</assets>
	<data type="item" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
		<asset>rasberry</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" prefix="limited_" ext="swf">
		<asset>rasberry</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" ext="xml" keep="true">
		<asset>lc_astronaut2</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" ext="swf" keep="true">
		<asset>lc_astronaut2</asset>
	</assets>
</manifest>