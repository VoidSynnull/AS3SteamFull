<bundle>
	<id>knights</id>
	<name>Knights Pack</name>
	
	<cardSets>
		<cardSet id="shared">
			<card>3003</card>
			<card>3304</card>
		</cardSet>
		<cardSet id="male">
			<card>3104</card>
		</cardSet>
		<cardSet id="female">
			<card>3105</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>0</bundleNum>
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>0</brightness>
	   			<contrast>0</contrast>
	   			<saturation>0</saturation>
	   			<hue>-10</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/knights.swf</assetPath>
	    </asset> 
    </assets>
</bundle>