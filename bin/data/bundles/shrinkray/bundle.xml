<bundle>
	<id>shrinkray</id>
	<name>Shrink Ray Pack</name>
	
	<cardSets>
		<cardSet id="shared">
			<card>3373</card>
			<card>3374</card>
			<card>3225</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>0</bundleNum>
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>-40</brightness>
	   			<contrast>13</contrast>
	   			<saturation>-57</saturation>
	   			<hue>-59</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/shrinkray.swf</assetPath>
	    </asset> 
    </assets>
</bundle>