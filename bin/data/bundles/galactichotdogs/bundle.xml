<bundle>
	<id>galactichotdogs</id>
	<name>Galactic Hot Dogs Pack</name>
	
	<cardSets>
		<cardSet id="shared">
			<card>3127</card>
			<card>3126</card>
			<card>3380</card>
			<card>3381</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>0</bundleNum>
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
