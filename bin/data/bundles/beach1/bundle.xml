<bundle>
	<id>beach1</id>
	<name>Tropical Pack</name>

	<cardSets>
		<cardSet id="shared">
			<card>3554</card>
			<card>3557</card>
		</cardSet>
		<cardSet id="male">
			<card>3550</card>
			<card>3552</card>
		</cardSet>
		<cardSet id="female">
			<card>3551</card>
			<card>3553</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>1</bundleNum>
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>-27</brightness>
	   			<contrast>0</contrast>
	   			<saturation>-27</saturation>
	   			<hue>-31</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/star.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>