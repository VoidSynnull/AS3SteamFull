<manifest>
	<!-- campaign xml files -->
	<data path="data/limited/Croods2AutoCard/" type="campaign" ext="xml">
		<asset>zipmanifest</asset>
	</data>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data path="data/scenes/limited/Croods2Quest_Game/" type="game" ext="xml">
		<asset>game</asset>
		<asset>hits</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets path="assets/scenes/limited/Croods2Quest_Game/" type="game" ext="swf">
		<asset>background1</asset>
		<asset>background2</asset>
		<asset>interactive</asset>
		<asset>foreground1</asset>
		<asset>foreground2</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets path="assets/limited/Croods2Quest/" type="support" ext="swf">
