<item id="2617"> <!-- Change this card ID -->
  	<type>custom</type>
	<subType>general</subType>
	<name>Video Card</name> <!-- Change this card name -->
  	<campaignID>MobileVideoTest</campaignID> <!-- Change this CMS name -->
	
  	<buttons>
  		<btn>
  			<index>1</index>
    		<label>PLAY VIDEO</label>
    		<actions>
				<action type="openPopup" tracking="Use Video Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="className">game.scenes.custom.CardPlayMobileVideo</param>
	  					<param id="video">video/GalacticHotDogs_update_960x540.flv</param> <!-- name of video -->
	  					<param id="clickURL">http://www.google.com</param>
					</parameters>
				</action>
			</actions>
  		</btn>
    	<btn>
    		<index>0</index>
      		<label>VISIT WEBSITE</label>
      		<actions>
				<action type="gotoUrl" tracking="Video Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="urlId">url1</param> <!-- NOTE :: This should get pulled from campaign.xml -->
					</parameters>
				</action>
	  		</actions>
    	</btn>
  	</buttons>
	
  	<textfields>
		<text id="limited">
			<value>FOR A LIMITED TIME ONLY</value>
		</text>
		<text id="subtitle">
			<value>Mobile Video Test</value> <!-- Change this subtitle -->
		</text>
		<text id="adtitle">
			<value>Video Card</value> <!-- Change this title -->
		</text>
		<text id="ad">
			<value>ADVERTISEMENT</value>
		</text>
  	</textfields>
	
  	<assets>
		<asset id="cardBack">
    		<assetPath>items/limited/backs/AdBackgroundBlue.swf</assetPath> <!-- Change this background swf -->
    	</asset>
  	</assets>
  	
</item>
