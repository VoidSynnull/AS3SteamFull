<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/CaptUnderpantsQuest/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
		<asset>version</asset> <!-- remove this if no new code added for this campaign -->
	</data>

		<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/CaptUnderpantsQuest_Interior/">
		<asset>dialog.xml</asset>
		<asset>doors.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>sounds.xml</asset>
		<asset>tracking.xml</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/CaptUnderpantsQuest_Interior/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/CaptUnderpantsQuest/" ext="swf">
		<asset>StartSynopsis1</asset>
		<asset>StartSynopsis2</asset>
		<asset>StartSynopsis3</asset>
		<asset>StartTrigger</asset>
		<asset>Harold</asset>
		<asset>George</asset>
	</assets>

	<assets type="game" path="assets/limited/CaptUnderpantsMMSQ/">
		<asset>CaptUnderpants.swf</asset>
	</assets>

	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
	</assets>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>ad_interior_scene</asset>
	</assets>
	
	<!-- avatar parts -->
	<data type="pack" path="data/entity/character/parts/pack/" ext="xml" keep="true">
		<asset>capt_underpants_cape</asset>
	</data>
	<assets type="pack" path="assets/entity/character/pack/" ext="swf" keep="true">
		<asset>capt_underpants_cape</asset>
	</assets>
</manifest>