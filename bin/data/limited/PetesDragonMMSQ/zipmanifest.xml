<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/PetesDragonMMSQ/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/PetesDragonMMSQ/mainStreet/">
		<asset>scene.xml</asset>
		<asset>dialog.xml</asset>
		<asset>doors.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>tracking.xml</asset>
		<asset>custom.xml</asset>
		<asset>npcs.xml</asset>
	</data>
	
	<assets type="support" path="assets/limited/PetesDragonMMSQ/mainStreet/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>
	
		
	<!-- avatar parts -->
	<data type="mouth" path="data/entity/character/parts/mouth/" ext="xml">
		<asset>magician</asset>
	</data>
	<assets type="mouth" path="assets/entity/character/mouth/" ext="swf">
		<asset>magician</asset>
	</assets>
	<data type="hair" path="data/entity/character/parts/hair/" ext="xml">
		<asset>limited_pete</asset>
	</data>
	<assets type="hair" path="assets/entity/character/hair/" ext="swf">
		<asset>limited_pete</asset>
	</assets>
	<data type="marks" path="data/entity/character/parts/marks/" ext="xml">
		<asset>ad_boxtrolls_eggs</asset>
	</data>
	<assets type="marks" path="assets/entity/character/marks/" ext="swf">
		<asset>ad_boxtrolls_eggs</asset>
	</assets>
	<data type="shirt" path="data/entity/character/parts/shirt/" ext="xml">
		<asset>bare</asset>		
	</data>
	<assets type="shirt" path="assets/entity/character/shirt/" ext="swf">
		<asset>bare</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" ext="xml">
		<asset>limited_spot</asset>		
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" ext="swf">
		<asset>limited_spot</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" prefix="limited_" ext="xml">
		<asset>pete</asset>
	</data>
	<assets type="pants" path="assets/entity/character/pants/" prefix="limited_" ext="swf">
		<asset>pete</asset>
	</assets>
	
</manifest>