<sounds>
	<sound type="ambient">
		<asset loop="true" faded="true" exclusiveType="false">city_night_time.mp3</asset>
	</sound>
	
	<sound type="effects">
		<asset triggeredByEvent="crane_running" loop="true">crane_engine_01_L.mp3</asset>
		<asset triggeredByEvent="crane_start">turn_engine_on_01.mp3</asset>
		<asset triggeredByEvent="crane_stop">turn_engine_off_01.mp3</asset>
		<asset triggeredByEvent="crane_grab_beam">magnet_metal_lock_01.mp3</asset>
		<asset triggeredByEvent="crane_running_heavy" loop="true">crane_engine_02_L.mp3</asset>
		<asset triggeredByEvent="crane_place_beam">metal_impact_05.mp3</asset>
		
		<!-- Lunch Box Mini Game -->
		<asset triggeredByEvent="move_chips">pick_up_wrapper_01.mp3</asset>
		<asset triggeredByEvent="move_wrapper">pick_up_wrapper_02.mp3</asset>
		<asset triggeredByEvent="move_banana_peel">pick_up_squishy_01.mp3</asset>
		<asset triggeredByEvent="place_item">put_misc_item_down_01.mp3</asset>
	</sound>
	
	<sound type="effects" action="doorOpened">
		<asset id="shopExit">door_pushbar_open_01.mp3</asset>
	</sound>
	
	<sound type="effects" action="impact">
		<asset id="roof">ls_concrete_01.mp3, ls_concrete_02.mp3</asset>
		<asset id="barrier">ls_wood_plank_01.mp3, ls_wood_plank_02.mp3</asset>
		<asset id="rail">ls_thin_metal_01.mp3, ls_thin_metal_02.mp3</asset>
		<asset id="bag">none</asset>
		<asset id="metal">ls_heavy_metal_01.mp3, ls_heavy_metal_02.mp3</asset>
		<asset id="block">ls_brick_01.mp3, ls_brick_02.mp3</asset>
		<asset id="bounce">ls_spring_01.mp3, ls_spring_02.mp3, ls_spring_03.mp3</asset>
		<asset id="baseGround">ls_concrete_01.mp3, ls_concrete_02.mp3</asset>
	</sound>
	
	<sound type="effects" action="step">
		<asset id="roof">fs_concrete_01.mp3, fs_concrete_02.mp3, fs_concrete_03.mp3, fs_concrete_04.mp3</asset>
		<asset id="barrier">fs_wood_plank_01.mp3, fs_wood_plank_02.mp3, fs_wood_plank_03.mp3, fs_wood_plank_04.mp3</asset>
		<asset id="rail">fs_thin_metal_01.mp3, fs_thin_metal_02.mp3, fs_thin_metal_03.mp3, fs_thin_metal_04.mp3</asset>
		<asset id="bag">none</asset>
		<asset id="metal">fs_heavy_metal_01.mp3, fs_heavy_metal_02.mp3, fs_heavy_metal_03.mp3, fs_heavy_metal_04.mp3</asset>
		<asset id="block">fs_brick_01.mp3, fs_brick_02.mp3, fs_brick_03.mp3, fs_brick_04.mp3</asset>
		<asset id="baseGround">fs_concrete_01.mp3, fs_concrete_02.mp3, fs_concrete_03.mp3, fs_concrete_04.mp3</asset>
	</sound>
</sounds>