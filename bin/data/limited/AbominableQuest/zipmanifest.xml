<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/AbominableQuest/" ext="xml">
		<asset>zipmanifest</asset>
	</data>

		<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/AbominableQuest_Interior/" ext="xml">
		<asset>dialog</asset>
		<asset>doors</asset>
		<asset>events</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/AbominableQuest_Interior/" ext="swf">
		<asset>backdrop</asset>
		<asset>background</asset>
		<asset>interactive</asset>
		<asset>foreground</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/AbominableQuest_Game/" ext="xml">
		<asset>game</asset>
		<asset>hits</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/AbominableQuest_Game/" ext="swf">
		<asset>background1</asset>
		<asset>background2</asset>
		<asset>interactive</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/AbominableQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>blank</asset>
		<asset>jin</asset>
		<asset>pen</asset>
		<asset>yetinpc</asset>
		<asset>yi</asset>
	</assets>

	<!-- followers -->
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_abominable</asset>
	</data>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2976</asset>
		<asset>2977</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2976</asset>
		<asset>2977</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundBlue</asset>
	</assets>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>ad_interior_scene</asset>
		<asset>space_quest</asset>
		<asset>galactic_hot_dogs_main_theme</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
	</assets>

	<!-- avatar parts -->
	<data type="facial" path="data/entity/character/parts/facial/" ext="xml">
		<asset>limited_abominable_berry</asset>
		<asset>limited_abominable_yeti</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" ext="swf">
		<asset>limited_abominable_berry</asset>
		<asset>limited_abominable_yeti</asset>
	</assets>
	<data type="item" path="data/entity/character/parts/item/" ext="xml">
		<asset>limited_abominable_berry</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" ext="swf">
		<asset>limited_abominable_berry</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" ext="xml">
		<asset>limited_abominable_yeti</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" ext="swf">
		<asset>limited_abominable_yeti</asset>
	</assets>
</manifest>