<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/BornInChinaMVU/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/BornInChinaMVU/mainStreet/">
		<asset>custom.xml</asset>
		<asset>dialog.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>tracking.xml</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/BornInChinaMVU/mainStreet/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2835</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2835</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundGreen</asset>
	</assets>

	<!-- followers -->
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_baby_panda</asset>
	</data>

	<!-- bitmap npcs -->
	<assets type="game" path="assets/limited/BornInChinaMVU/">
		<asset>panda.swf</asset>
	</assets>

	<!-- avatar parts -->
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>babypanda</asset>
		<asset>panda_mom</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>babypanda</asset>
		<asset>panda_mom</asset>
	</assets>
	<data type="pack" path="data/entity/character/parts/pack/" prefix="limited_" ext="xml">
		<asset>babypanda</asset>
	</data>
	<assets type="pack" path="assets/entity/character/pack/" prefix="limited_" ext="swf">
		<asset>babypanda</asset>
	</assets>
	<data type="item" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
		<asset>bamboo</asset>
		<asset>bamboo_recover</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" prefix="limited_" ext="swf">
		<asset>bamboo</asset>
		<asset>bamboo_recover</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" prefix="limited_" ext="xml">
		<asset>babypanda</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" prefix="limited_" ext="swf">
		<asset>babypanda</asset>
	</assets>
</manifest>