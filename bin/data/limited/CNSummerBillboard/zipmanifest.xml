<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/CNSummerBillboard/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset> <!-- remove this if billboard awards cards -->
		<asset>version</asset>
	</data>

	<!-- side street xml files and swfs -->
