<!-- set "Click through URL" field in CMS to campaign.xml to support handling of cards -->
<campaign>
  	<campaignId>HotelTrans3MVU</campaignId> <!-- *** Must match campaign folder name -->
  	<version>2</version> <!-- version number that doesn't use steet paths -->
	<lockVideo>true</lockVideo> <!-- overrides locked video flag in hotspots.xml -->
 	
	<clickUrls>
		<clickUrl id="Default">https://ad.doubleclick.net/ddm/clk/427580868;229199213;v</clickUrl> <!-- Default clickURL as catch-all (use ad building clickURL)-->
		<clickUrl id="Video">https://ad.doubleclick.net/ddm/clk/427580865;229198619;b</clickUrl> <!-- for clicking on visit sponsor button on video end screen -->
		<clickUrl id="Card">https://ad.doubleclick.net/ddm/clk/427580874;229464981;z</clickUrl> <!-- for clicking on visit sponsor button on cards -->
	</clickUrls>
	
	<!-- third-party impressions by event name -->
	<impressionUrls>
		<impressions id="AdImpression"> <!-- For exterior ad building appeaing on main street -->
			<impression type="standard">https://ad.doubleclick.net/ddm/ad/N5811.3392561ORION/B21651853.229199213;sz=1x1;ord=[timestamp];dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=?</impression>
		</impressions>
		<impressions id="VideoClicked"> <!-- For playing video -->
			<impression type="standard">https://ad.doubleclick.net/ddm/ad/N5811.3392561ORION/B21651853.229198619;sz=1x1;ord=[timestamp];dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=?</impression>
		</impressions>
		<impressions id="VideoReplayed"> <!-- For replaying video (usually same tag as for VideoClicked) -->
			<impression type="standard">https://ad.doubleclick.net/ddm/ad/N5811.3392561ORION/B21651853.229198619;sz=1x1;ord=[timestamp];dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=?</impression>
		</impressions>
		<impressions id="NPCImpression"> <!-- When NPC appears in front of ad building or in interior (specify in tracking.xml) -->
			<impression type="standard">https://ad.doubleclick.net/ddm/ad/N5811.3392561ORION/B21651853.229465239;sz=1x1;ord=[timestamp];dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=?</impression>
		</impressions>
		<impressions id="CardImpression"> <!-- When prize card appears when awarded or in inventory -->
			<impression type="standard">https://ad.doubleclick.net/ddm/ad/N5811.3392561ORION/B21651853.229464981;sz=1x1;ord=[timestamp];dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=?</impression>
		</impressions>
	</impressionUrls>
	
	<boycards>
		<card>2921</card>
	</boycards>
	<girlcards>
		<card>2921</card>
	</girlcards>
</campaign>
