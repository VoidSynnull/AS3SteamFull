<bundle>
	<id>halloween1</id>
	<name>Halloween: Furry Monsters Pack</name>
	
	<cardSets>
		<cardSet id="shared"/>
		<cardSet id="male">
			<card>3149</card>
			<card>3150</card>
			<card>3151</card>
		</cardSet>
		<cardSet id="female">
			<card>3153</card>
			<card>3154</card>
			<card>3155</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>1</bundleNum>
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>0</brightness>
	   			<contrast>0</contrast>
	   			<saturation>-28</saturation>
	   			<hue>22</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/pumpkin.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>