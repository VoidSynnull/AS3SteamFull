<bundle>
	<id>predator</id>
	<name>Predator Pack</name>
	
	<cardSets>
		<cardSet id="shared">
			<card>3084</card>
			<card>3013</card>
			<card>3004</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>0</bundleNum>
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>-10</brightness>
	   			<contrast>0</contrast>
	   			<saturation>12</saturation>
	   			<hue>-20</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/predator.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>