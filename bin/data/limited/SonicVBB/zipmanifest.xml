<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/Frozen2VBB/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/Frozen2VBB/" ext="xml">
		<asset>custom</asset>
		<asset>dialog</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/Frozen2VBB/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
		<asset>Anna</asset>
		<asset>Elsa</asset>
		<asset>petal</asset>
		<asset>lift_spirits</asset>
	</assets>
	
	<!-- followers -->
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_olaf</asset>
	</data>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2983</asset>
		<asset>2984</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2983</asset>
		<asset>2984</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundTeal</asset>
	</assets>

	<!-- avatar parts -->
	<data type="pants" path="data/entity/character/parts/item/" prefix="limited_" ext="xml">
		<asset>lift_spirit</asset>
	</data>
	<assets type="pants" path="assets/entity/character/item/" prefix="limited_" ext="swf">
		<asset>lift_spirit</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" prefix="limited_" ext="xml">
		<asset>olaf</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" prefix="limited_" ext="swf">
		<asset>olaf</asset>
	</assets>
</manifest>