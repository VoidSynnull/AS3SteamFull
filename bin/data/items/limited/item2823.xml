<item id="2823"> <!-- Change this card ID -->
  	<type>custom</type>
	<subType>general</subType>
	<name>Lego Wishlist 2016 Card</name> <!-- Change this card name -->
  	<campaignID>LegoWishlist2016</campaignID> <!-- Change this CMS name -->
	
  	<buttons>
  		<btn>
  			<index>1</index>
    		<label>USE</label>
    		<actions>
				<action type="gotoUrl" tracking="Use Lego Wishlist 2016 Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="urlId">url2</param> <!-- NOTE :: This should get pulled from campaign.xml -->
					</parameters>
				</action>
			</actions>
  		</btn>
    	<btn>
    		<index>0</index>
      		<label>VISIT SITE</label>
      		<actions>
				<action type="gotoUrl" tracking="Lego Wishlist 2016 Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="urlId">url1</param> <!-- NOTE :: This should get pulled from campaign.xml -->
					</parameters>
				</action>
	  		</actions>
    	</btn>
  	</buttons>
	
	<conditionals>
    	<conditional type="hasLook" />
    </conditionals>
    
	
  	<textfields>
		<text id="limited">
			<value>FOR A LIMITED TIME ONLY</value>
		</text>
		<text id="subtitle">
			<value>Lego Friends</value> <!-- Change this subtitle -->
		</text>
		<text id="adtitle">
			<value>Wishlist</value> <!-- Change this title -->
		</text>
		<text id="ad">
			<value>ADVERTISEMENT</value>
		</text>
  	</textfields>
	
  	<assets>
		<asset id="cardBack">
    		<assetPath>items/limited/backs/AdBackgroundPurple.swf</assetPath> <!-- Change this background swf -->
    	</asset>
		 <asset id="cardContent" /> <!-- Will use content bin/assets/items/limited/item999.swf -->
  	</assets>
    

</item>
