<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/LegoMovie2Quest/" ext="xml">
		<asset>zipmanifest</asset>
	</data>

		<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/LegoMovie2Quest_Interior/" ext="xml">
		<asset>dialog</asset>
		<asset>doors</asset>
		<asset>events</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/LegoMovie2Quest_Interior/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
		<asset>foreground</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/LegoMovie2Quest_Game/" ext="xml">
		<asset>game</asset>
		<asset>hits</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/LegoMovie2Quest_Game/" ext="swf">
		<asset>background1</asset>
		<asset>background2</asset>
		<asset>interactive</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/LegoMovie2Quest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>Choose</asset>
		<asset>kitty</asset>
		<asset>planty</asset>
		<asset>pirate</asset>
		<asset>pirateGame</asset>
		<asset>batman</asset>
		<asset>lucy</asset>
		<asset>emmet</asset>
	</assets>

	<!-- followers -->
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_angry_kitty</asset>
		<asset>pop_follower_planty</asset>
	</data>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2952</asset>
		<asset>2953</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2952</asset>
		<asset>2953</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundYellow</asset>
	</assets>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>galactic_hot_dogs_main_theme</asset>
		<asset>space_quest</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
	</assets>

	<!-- avatar parts -->
	<data type="overshirt" path="data/entity/character/parts/overshirt/" prefix="limited_" ext="xml">
		<asset>lego2_buggy_emmet</asset>
		<asset>lego2_buggy_lucy</asset>
		<asset>lego2_planty</asset>
		<asset>lego2_angry_kitty</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" prefix="limited_" ext="swf">
		<asset>lego2_buggy_emmet</asset>
		<asset>lego2_buggy_lucy</asset>
		<asset>lego2_planty</asset>
		<asset>lego2_angry_kitty</asset>
	</assets>
</manifest>