<events>
	<group type="triggered" id="startGame">
		<event>
			<param1 id="action">callFunction</param1>
			<param1 id="function">selectArcadeGame</param1>
			<param1 id="game">StarShooter</param1>
		</event>
	</group>
	<group type="triggered" id="initScene">
		<event>
			<param1 id="action">talk</param1>
			<param2 id="npcID">courtney</param2>
			<param3 id="sayID">arcade</param3>
		</event>
	</group>
	<group type="triggered" id="initSceneReturn">
	</group> 
</events>