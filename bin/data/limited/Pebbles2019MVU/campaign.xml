<!-- set "Click through URL" field in CMS to campaign.xml to support handling of cards -->
<campaign>
  	<campaignId>Pebbles2019MVU</campaignId> <!-- *** Must match campaign folder name -->
  	<version>2</version> <!-- version number that doesn't use steet paths -->
	<lockVideo>true</lockVideo> <!-- overrides locked video flag in hotspots.xml -->
 	
	<clickUrls>
		<clickUrl id="Default">https://www.pebblescereal.com/</clickUrl> <!-- Default clickURL as catch-all (use ad building clickURL)-->
		<clickUrl id="Video">https://www.pebblescereal.com/</clickUrl> <!-- for clicking on visit sponsor button on video end screen -->
		<clickUrl id="Poster">https://www.pebblescereal.com/</clickUrl> <!-- for clicking on posters that take user to sponsor site -->
		<clickUrl id="Card">https://www.pebblescereal.com/</clickUrl> <!-- for clicking on visit sponsor button on cards -->`
	</clickUrls>
	
	<!-- third-party impressions by event name -->
	<impressionUrls>
		<impressions id="AdImpression"> <!-- For exterior ad building appeaing on main street -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
	</impressionUrls>
	
	<boycards>
		<card>2948</card>
		<card>2949</card>
	</boycards>
	<girlcards>
		<card>2948</card>
		<card>2949</card>
	</girlcards>
</campaign>