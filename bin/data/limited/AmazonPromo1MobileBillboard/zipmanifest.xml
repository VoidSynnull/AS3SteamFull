<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/AmazonPromo1MobileBillboard/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
	</data>

	<!-- side street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/AmazonPromo1MobileBillboard/sideStreet/">
		<asset>custom.xml</asset>
		<asset>scene.xml</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/AmazonPromo1MobileBillboard/sideStreet/">
		<asset>background.swf</asset>
	</assets>
</manifest>