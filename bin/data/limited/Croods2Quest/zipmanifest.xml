<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/Croods2Quest/" ext="xml">
		<asset>zipmanifest</asset>
	</data>

		<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/Croods2Quest_Interior/" ext="xml">
		<asset>dialog</asset>
		<asset>doors</asset>
		<asset>events</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
		<asset>tracking</asset>
	</data>
	<data type="support" path="data/map/" ext="xml">
		<asset>mobile_americangirlkira</asset>
		<asset>mobile_americangirl</asset>
	</data>
	<assets type="game" path="assets/limited/Croods2MMSQ/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/Croods2Quest_Interior/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
		<asset>foreground</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/Croods2Quest_Game/" ext="xml">
		<asset>game</asset>
		<asset>hits</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/Croods2Quest_Game/" ext="swf">
		<asset>background1</asset>
		<asset>background2</asset>
		<asset>interactive</asset>
		<asset>foreground1</asset>
		<asset>foreground2</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/Croods2Quest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>Belt</asset>
		<asset>Dawn</asset>
