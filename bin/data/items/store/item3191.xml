<item id="3191">
		<type>island</type>
		<subType>examples</subType>
		<buttons>
			<btn>
			<conditional id="hasAbility" isTrue="false"/>
				<label>USE</label>
				<actions>
				<action type="activatePower">
					<parameters>
								<param id="trigger">true</param>
					</parameters>
				</action>
			</actions>
			</btn>
			<btn>
				<conditional id="hasAbility" isTrue="true"/>
				<label>TURN OFF</label>
				<actions>
				<action type="deactivatePower"/>
			</actions>
			</btn>
		</buttons>
		<textfields>
			<text id="storeTitle">
					<value>Star Power</value>
	 	 	</text>
		</textfields>
		<assets>
		<asset id="cardBack" /><asset id="cardContent"/>
		</assets>
		
 	<specials>
			<specialAbility>stars_circle</specialAbility>
	</specials>
	
	<conditionals>
			<conditional type="hasAbility" />
		</conditionals>
		
</item>

