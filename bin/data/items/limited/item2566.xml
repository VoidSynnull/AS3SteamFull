<item id="2566"> <!-- Change this card ID -->
  	<type>custom</type>
	<subType>general</subType>
	<name>Balloon Game Card</name> <!-- Change this card name -->
  	<campaignID>PoptropicaComicsMMQ</campaignID> <!-- Change this CMS name -->
	
  	<buttons>
  		<btn>
  			<index>1</index>
    		<label>USE</label>
    		<actions>
				<action type="openPopup" tracking="Use Balloon Game Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="className">game.scenes.custom.TwitchGamePower</param>
	  					<param id="swfPath">PoptropicaComicsQuest/TwitchTest.swf</param> <!-- location of swf in assets/limited -->
						<param id="xmlPath">PoptropicaComicsQuest/TwitchTest.xml</param>
					</parameters>
				</action>
			</actions>
  		</btn>
    	<btn>
    		<index>0</index>
      		<label>VISIT WEBSITE</label>
      		<actions>
				<action type="gotoUrl" tracking="Balloon Game Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="urlId">url1</param> <!-- NOTE :: This should get pulled from campaign.xml -->
					</parameters>
				</action>
	  		</actions>
    	</btn>
  	</buttons>
	
  	<textfields>
		<text id="limited">
			<value>FOR A LIMITED TIME ONLY</value>
		</text>
		<text id="subtitle">
			<value>Poptropica Comics</value> <!-- Change this subtitle -->
		</text>
		<text id="adtitle">
			<value>Balloon Pop Game</value> <!-- Change this title -->
		</text>
		<text id="ad">
			<value>ADVERTISEMENT</value>
		</text>
		<text id="memberinstructions"> <!-- Change any member instructions -->
			
		</text>
  	</textfields>
	
  	<assets>
		<asset id="cardBack">
    		<assetPath>items/limited/backs/AdBackgroundBlue.swf</assetPath> <!-- Change this background swf -->
    	</asset>
		
  	</assets>
  	
</item>
