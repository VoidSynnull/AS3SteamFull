<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/PeanutsMovieMobileMVU/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/PeanutsMovieMobileMVU/mainStreet/">
		<asset>scene.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>tracking.xml</asset>
		<asset>custom.xml</asset>
		<asset>dialog.xml</asset>
		<asset>npcs.xml</asset>
		<asset>hits.xml</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/PeanutsMovieMobileMVU/mainStreet/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
	</assets>
	
	<assets type="support" path="assets/limited/PeanutsMovieMobileMVU/" ext="swf">
		<asset>CharlieBrown_NPC</asset>
		<asset>Follower</asset>
	</assets>
	
	<assets type="ability" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>follower_peanuts_woodstock</asset>
	</assets>
	
	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2725</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2725</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundBlue</asset>
	</assets>
	
</manifest>