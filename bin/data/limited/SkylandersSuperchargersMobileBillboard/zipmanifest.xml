<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/SkylandersSuperchargersMobileBillboard/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
	</data>

	<data type="campaign" path="data/limited/SkylandersSuperchargersMobileMVU/" ext="xml">
		<asset>campaign</asset>
	</data>
	
	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/SkylandersSuperchargersMobileBillboard/sideStreet/">
		<asset>scene.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>tracking.xml</asset>
		<asset>custom.xml</asset>
		<asset>dialog.xml</asset>
		<asset>npcs.xml</asset>
		<asset>hits.xml</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/SkylandersSuperchargersMobileBillboard/sideStreet/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2741</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2741</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundRed</asset>
	</assets>

	<assets type="support" path="assets/limited/SkylandersSuperchargersMobileMVU/">
		<asset>follower.swf</asset>
	</assets>
	<data type="ability" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>follower_hot_streak</asset>
	</data>

	<!-- avatar parts -->
	<data type="mouth" path="data/entity/character/parts/mouth/" prefix="limited_" ext="xml">
		<asset>leo</asset>
	</data>
	<assets type="mouth" path="assets/entity/character/mouth/" prefix="limited_" ext="swf">
		<asset>leo</asset>
	</assets>
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>skylanders_flynn</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>skylanders_flynn</asset>
	</assets>
	<data type="shirt" path="data/entity/character/parts/shirt/" prefix="limited_" ext="xml">
		<asset>skylanders_flynn</asset>
	</data>
	<assets type="shirt" path="assets/entity/character/shirt/" prefix="limited_" ext="swf">
		<asset>skylanders_flynn</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" prefix="limited_" ext="xml">
		<asset>skylanders_flynn</asset>
	</data>
	<assets type="pants" path="assets/entity/character/pants/" prefix="limited_" ext="swf">
		<asset>skylanders_flynn</asset>
	</assets>
</manifest>