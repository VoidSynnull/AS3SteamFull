<obstacles>
	<!-- list obstacles types here: moving, static, boost, slick, or placement -->
	<!-- each must have baseName and type -->
	<!-- obstacles are grouped with attributes for all enclosed items -->	
	<!-- attributes for individual items will override group attributes -->	
	<!-- all item names must end with integer and match instance names in the FLA file -->
	<obstacle baseName="mud" type="slick" slickTime="1.5">
		<item name="mud1" />
	</obstacle>
	<obstacle baseName="power" type="boost" boostSpeed="500" boostTime="2.0" >
		<item name="power1" points="150"/>
	</obstacle>
	<obstacle baseName="car" type="crashing" speed="400" hitTime="0.2" >
		<item name="car1" speed="420" points="100"/>
		<item name="car2" speed="390" points="100"/>
		<item name="car3" points="100"/>
