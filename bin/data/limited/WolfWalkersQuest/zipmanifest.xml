	<data type="overshirt" path="data/entity/character/parts/overshirt/" ext="xml">
		<asset>limited_mebhe_wolf_torso</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" ext="swf">
		<asset>limited_mebhe_wolf_torso</asset>
	</assets>
</manifest>