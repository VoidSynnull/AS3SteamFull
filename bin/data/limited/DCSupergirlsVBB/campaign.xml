<!-- set "Click through URL" field in CMS to campaign.xml to support handling of cards -->
<campaign>
  	<campaignId>DCSupergirlsVBB</campaignId> <!-- *** Must match campaign folder name -->
  	<version>2</version> <!-- version number that doesn't use steet paths -->
	<lockVideo>true</lockVideo> <!-- overrides locked video flag in hotspots.xml -->
 	
	<clickUrls>
		<clickUrl id="Default">https://www.cartoonnetwork.com/video/dc-super-hero-girls/index.html</clickUrl> <!-- Default clickURL as catch-all (use ad building clickURL)-->
		<clickUrl id="Video">https://www.cartoonnetwork.com/video/dc-super-hero-girls/index.html</clickUrl> <!-- for clicking on visit sponsor button on video end screen -->
		<clickUrl id="Poster">https://www.cartoonnetwork.com/video/dc-super-hero-girls/index.html</clickUrl> <!-- for clicking on posters that take user to sponsor site -->
		<clickUrl id="Card">https://www.cartoonnetwork.com/video/dc-super-hero-girls/index.html</clickUrl> <!-- for clicking on visit sponsor button on cards -->
	</clickUrls>
	
	<!-- third-party impressions by event name -->
	<impressionUrls>
		<impressions id="AdImpression"> <!-- For exterior ad building appeaing on main street -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
		<impressions id="VideoClicked"> <!-- For playing video -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
		<impressions id="VideoReplayed"> <!-- For replaying video (usually same tag as for VideoClicked) -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
		<impressions id="NPCImpression"> <!-- When NPC appears in front of ad building or in interior (specify in tracking.xml) -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
		<impressions id="CardImpression"> <!-- When prize card appears when awarded or in inventory -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
		<impressions id="StartBannerImpression"> <!-- for start popup appearing -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
		<impressions id="WinBannerImpression"> <!-- for win popup appearing -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
		<impressions id="LoseBannerImpression"> <!-- for lose popup appearing -->
			<impression type="standard">http://doubleclick.net?param=blank</impression>
		</impressions>
	</impressionUrls>
	
	<boycards>
		<card>2960</card>
		<card>2961</card>
	</boycards>
	<girlcards>
		<card>2960</card>
		<card>2961</card>
	</girlcards>
</campaign>
