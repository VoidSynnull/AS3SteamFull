<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/LegoBatmanQuest/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
		<asset>version</asset>
	</data>

	<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/LegoBatmanQuest_Interior/">
		<asset>dialog.xml</asset>
		<asset>doors.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>sounds.xml</asset>
		<asset>tracking.xml</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/LegoBatmanQuest_Interior/">
		<asset>backdrop.swf</asset>
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
		<asset>foreground.swf</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/LegoBatmanQuest_Game/">
		<asset>game.xml</asset>
		<asset>hits.xml</asset>
		<asset>motionMaster.xml</asset>
		<asset>scene.xml</asset>
		<asset>segmentPatterns.xml</asset>
		<asset>sounds.xml</asset>
	</data>
		<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/LegoBatmanQuest_Game/">
		<asset>interactive.swf</asset>
	</assets>

	
	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/LegoBatmanQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
	</assets>
	
	
	<assets type="support" path="assets/limited/LegoBatmanQuest/" ext="swf">
		<asset>Batman</asset>
		<asset>Robin</asset>
		<asset>Barbera</asset>
		<asset>grapple</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2827</asset>
		<asset>2828</asset>
	</data>
	<data type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2827</asset>
		<asset>2828</asset>
	</data>
	
		<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundOrange</asset>
	</assets>

	<!-- animation xml files -->
	<data type="animation" path="data/entity/character/animation/human/" ext="xml" keep="true">
		<asset>standNinja</asset>
	</data>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>mighty_action_force</asset>
		<asset>epic_boss_battle</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
		<asset>ls_concrete_01</asset>
		<asset>ls_concrete_02</asset>
		<asset>air_shot_03</asset>
		<asset>explosion_02</asset>
		<asset>explosion_04</asset>
		<asset>big_pop_01</asset>
		<asset>big_pow_05</asset>
		<asset>door_hatch_01</asset>
		<asset>whoosh_05</asset>
		<asset>ls_bounce_01</asset>
		<asset>ls_bounce_02</asset>
		<asset>ls_bounce_03</asset>
		<asset>explosion_01</asset>
	</assets>

	<data type="item" path="data/entity/character/parts/item/" ext="xml">
		<asset>limited_batarang</asset>
	</data>
	<assets type="item" path="assets/entity/character/item/" ext="swf">
		<asset>limited_batarang</asset>
	</assets>
	<data type="overpants" path="data/entity/character/parts/overpants/" ext="xml">
		<asset>limited_legobatman_belt_grapple</asset>
	</data>
	<assets type="overpants" path="assets/entity/character/overpants/" ext="swf">
		<asset>limited_legobatman_belt_grapple</asset>
	</assets>

</manifest>