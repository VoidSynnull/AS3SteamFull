<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/WonderparkQuest/" ext="xml">
		<asset>zipmanifest</asset>
	</data>

		<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/WonderparkQuest_Interior/" ext="xml">
		<asset>dialog</asset>
		<asset>doors</asset>
		<asset>events</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/WonderparkQuest_Interior/" ext="swf">
		<asset>backdrop</asset>
		<asset>background</asset>
		<asset>interactive</asset>
		<asset>foreground</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/WonderparkQuest_Game/" ext="xml">
		<asset>game</asset>
		<asset>hits</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/WonderparkQuest_Game/" ext="swf">
		<asset>background1</asset>
		<asset>background2</asset>
		<asset>interactive</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/WonderparkQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>follower</asset>
		<asset>boomer</asset>
		<asset>june</asset>
		<asset>greta</asset>
	</assets>

	<!-- followers -->
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_boomer</asset>
	</data>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2950</asset>
		<asset>2951</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2950</asset>
		<asset>2951</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundYellow</asset>
	</assets>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>Drive</asset>
		<asset>ringmasters_tent</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
		<asset>cart_bump_01</asset>
		<asset>small_pow_05</asset>
	</assets>

	<data type="overshirt" path="data/entity/character/parts/overshirt/" prefix="limited_" ext="xml">
		<asset>wonderpark_car</asset>
		<asset>wonderpark_car_game</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" prefix="limited_" ext="swf">
		<asset>wonderpark_car</asset>
		<asset>wonderpark_car_game</asset>
	</assets>
</manifest>