<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/BFGMMQ/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset>
		<asset>version</asset>
		<asset>map/island</asset>
		<asset>map/island/page</asset>
	</data>

	<!-- map swfs -->
	<assets type="island" path="assets/limited/BFGMMQ/map/" ext="swf">
		<asset>icon</asset>
		<asset>island</asset>
		<asset>logo</asset>
		<asset>island/background</asset>
		<asset>island/slides</asset>
	</assets>
	
	<assets type="island" path="assets/scenes/map/map/layouts/" ext="swf">
		<asset>advertisement</asset>
	</assets>
	
	<data type="island" path="data/scenes/map/map/layouts/" ext="xml">
		<asset>advertisement</asset>
	</data>

</manifest>