<bundle>
	<id>halloween3</id>
	<name>Halloween: Creepy Monsters Pack</name>

	<cardSets>
		<cardSet id="shared">
			<card>3368</card>
			<card>3367</card>
		</cardSet>
		<cardSet id="male">
			<card>3062</card>
		</cardSet>
		<cardSet id="female">
			<card>3061</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>3</bundleNum>
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>-27</brightness>
	   			<contrast>0</contrast>
	   			<saturation>-27</saturation>
	   			<hue>-31</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/pumpkin.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>