		<asset>gh_humphree</asset>
	</assets>
	<data type="shirt" path="data/entity/character/parts/shirt/" prefix="limited_" ext="xml">
		<asset>gh_boy2</asset>
	</data>
	<assets type="shirt" path="assets/entity/character/shirt/" prefix="limited_" ext="swf">
		<asset>gh_boy2</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" prefix="limited_" ext="xml">
		<asset>gh_boy2</asset>
		<asset>gh_humphree</asset>
	</data>
	<assets type="pants" path="assets/entity/character/pants/" prefix="limited_" ext="swf">
		<asset>gh_boy2</asset>
		<asset>gh_humphree</asset>
	</assets>
	<data type="overshirt" path="data/entity/character/parts/overshirt/" prefix="limited_" ext="xml">
		<asset>gh_boy2</asset>
		<asset>gh_humphree</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" prefix="limited_" ext="swf">
		<asset>gh_boy2</asset>
