		<text id="adtitle">
			<value>Fruit Geyser Power</value> <!-- Change this title -->
			<y>-195</y>
		</text>
		<text id="ad">
			<value>ADVERTISEMENT</value>
		</text>
		<text id="memberinstructions"> <!-- Change any member instructions -->
			<value></value>
			<y>66</y>
		</text>
		<text id="copyright">
			<value></value> <!-- Change or delete this copyright -->
		</text>
  	</textfields>
	
  	<assets>
		<asset id="cardBack">
    		<assetPath>items/limited/backs/AdBackgroundBlue.swf</assetPath> <!-- Change this background swf -->
    	</asset>
		<asset id="cardContent" />  
  	</assets>
  	
</item>
