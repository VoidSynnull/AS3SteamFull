<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/LegoCityVendorCart/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/LegoCityVendorCart/" ext="xml">
		<asset>custom</asset>
		<asset>dialog</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/LegoCityVendorCart/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>
	
	<!-- bitmap npcs -->
	<assets type="game" path="assets/limited/LegoCityQuest/" ext="swf">
		<asset>npc1</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2913</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2913</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundBlue</asset>
	</assets>
	<!-- avatar parts -->
	<data type="overshirt" path="data/entity/character/parts/overshirt/" ext="xml">
		<asset>limited_legocity_snowmobile</asset>
	</data>
	<assets type="overshirt" path="assets/entity/character/overshirt/" ext="swf">
		<asset>limited_legocity_snowmobile</asset>
	</assets>
</manifest>