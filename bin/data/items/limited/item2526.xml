  	<buttons>
  		<btn>
  			<index>1</index>
    		<label>USE</label>
    		<actions>
				<action type="openPopup" tracking="Use Fruit Geyser Power Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="className">game.scenes.custom.CardAnimPower</param>
	  					<param id="swfPath">CapriSun2014Quest/fruitPower.swf</param> <!-- location of swf in assets/limited -->
					</parameters>
				</action>
			</actions>
  		</btn>
    	<btn>
    		<index>0</index>
      		<label>VISIT WEBSITE</label>
      		<actions>
				<action type="gotoUrl" tracking="Fruit Geyser Power Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="urlId">url1</param> <!-- NOTE :: This should get pulled from campaign.xml -->
					</parameters>
				</action>
	  		</actions>
    	</btn>
  	</buttons>
	
  	<textfields>
		<text id="limited">
			<value>FOR A LIMITED TIME ONLY</value>
		</text>
		<text id="subtitle">
			<value></value> <!-- Change this subtitle -->
