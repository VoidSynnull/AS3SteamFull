<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/SherlockGnomesQuest/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>campaign</asset> <!-- need this to override version copied to local storage -->
	</data>

		<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/SherlockGnomesQuest_Interior/" ext="xml">
		<asset>dialog</asset>
		<asset>doors</asset>
		<asset>events</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/SherlockGnomesQuest_Interior/" ext="swf">
		<asset>backdrop</asset>
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/SherlockGnomesQuest_Game/" ext="xml">
		<asset>game</asset>
		<asset>hits</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>sounds</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/SherlockGnomesQuest_Game/" ext="swf">
		<asset>background</asset>
		<asset>foreground</asset>
		<asset>interactive</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/SherlockGnomesQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
		<asset>Sherlock</asset>
		<asset>Gnomeo</asset>
		<asset>Juliet</asset>
	</assets>

	<!-- followers -->
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_gnomeo</asset>
		<asset>pop_follower_juliet</asset>
	</data>

	<!-- poster -->
	<data type="poster" path="assets/posters/" ext="swf">
		<asset>GnomeoPoster</asset>
	</data>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2879</asset>
		<asset>2880</asset>
		<asset>2881</asset>
		<asset>2882</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2879</asset>
		<asset>2880</asset>
		<asset>2881</asset>
		<asset>2882</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundGreen</asset>
	</assets>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>ad_interior_scene</asset>
		<asset>collection_game</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
		<asset>ls_bounce_01</asset>
		<asset>ls_bounce_02</asset>
		<asset>ls_bounce_03</asset>
	</assets>

	<!-- avatar parts -->
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>gnomeo</asset>
		<asset>juliet</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>gnomeo</asset>
		<asset>juliet</asset>
	</assets>
	<data type="shirt" path="data/entity/character/parts/shirt/" prefix="limited_" ext="xml">
		<asset>gnomeo</asset>
		<asset>juliet</asset>
	</data>
	<assets type="shirt" path="assets/entity/character/shirt/" prefix="limited_" ext="swf">
		<asset>gnomeo</asset>
		<asset>juliet</asset>
	</assets>
	<data type="hair" path="data/entity/character/parts/hair/" prefix="limited_" ext="xml">
		<asset>gnomehat</asset>
	</data>
	<assets type="hair" path="assets/entity/character/hair/" prefix="limited_" ext="swf">
		<asset>gnomehat</asset>
	</assets>
</manifest>