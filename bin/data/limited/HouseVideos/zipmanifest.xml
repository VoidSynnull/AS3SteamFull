<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/HouseVideos/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/HouseVideos/" ext="xml">
		<asset>custom</asset>
		<asset>dialog</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>tracking</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/HouseVideos/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>
	
	<!-- avatar parts -->
	<data type="mouth" path="data/entity/character/parts/mouth/" ext="xml">
		<asset>sponsortsbralien</asset>
	</data>
	<assets type="mouth" path="assets/entity/character/mouth/" ext="swf">
		<asset>sponsortsbralien</asset>
	</assets>
	<data type="marks" path="data/entity/character/parts/marks/" keep="true" ext="xml">
		<asset>freckles</asset>
	</data>
	<assets type="marks" path="assets/entity/character/marks/" keep="true" ext="swf">
		<asset>freckles</asset>
	</assets>
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>percyjackson_cyclops</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>percyjackson_cyclops</asset>
	</assets>
	<data type="hair" path="data/entity/character/parts/hair/" keep="true" ext="xml">
		<asset>comic_jorge</asset>
	</data>
	<assets type="hair" path="assets/entity/character/hair/" keep="true" ext="swf">
		<asset>comic_jorge</asset>
	</assets>
	<data type="shirt" path="data/entity/character/parts/shirt/" keep="true" ext="xml">
		<asset>z_vidgme</asset>
	</data>
	<assets type="shirt" path="assets/entity/character/shirt/" keep="true" ext="swf">
		<asset>z_vidgme</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" keep="true" ext="xml">
		<asset>shendiva</asset>
	</data>
	<assets type="pants" path="assets/entity/character/pants/" keep="true" ext="swf">
		<asset>shendiva</asset>
	</assets>
</manifest>