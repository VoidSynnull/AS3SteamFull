<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/NineLivesQuest/" ext="xml">
		<asset>campaign</asset>
		<asset>zipmanifest</asset>
	</data>
	
	<!-- interior xml files and swfs -->
	<!-- from scene.xml in interior scene plus scene.xml -->
	<data type="interior" path="data/scenes/limited/NineLivesQuest_Interior/">
		<asset>doors.xml</asset>
		<asset>dialog.xml</asset>
		<asset>events.xml</asset>
		<asset>hits.xml</asset>
		<asset>hotspots.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>sounds.xml</asset>
		<asset>tracking.xml</asset>
	</data>
	<!-- from scene.xml in interior scene -->
	<assets type="interior" path="assets/scenes/limited/NineLivesQuest_Interior/">
		<asset>background.swf</asset>
		<asset>interactive.swf</asset>
		<asset>foreground.swf</asset>
	</assets>

	<!-- game xml files and swfs -->
	<!-- from scene.xml in game scene plus scene.xml -->
	<data type="game" path="data/scenes/limited/NineLivesQuest_Game/">
		<asset>game.xml</asset>
		<asset>hits.xml</asset>
		<asset>npcs.xml</asset>
		<asset>scene.xml</asset>
		<asset>sounds.xml</asset>
	</data>
	<!-- from scene.xml in game scene -->
	<assets type="game" path="assets/scenes/limited/NineLivesQuest_Game/" ext="swf">
		<asset>start_background</asset>
		<asset>background</asset>
		<asset>background2</asset>
		<asset>interactive</asset>
	</assets>

	<!-- popups, followers, bitmap NPCs -->
	<assets type="support" path="assets/limited/NineLivesQuest/" ext="swf">
		<asset>Start</asset>
		<asset>Win</asset>
		<asset>Lose</asset>
	</assets>
	
	<!-- followers, -->
	<data type="follower" path="data/entity/character/abilities/limited/" ext="xml">
		<asset>pop_follower_9lives_fuzzypants</asset>
	</data>

	<!-- creature files -->
	<data type="creature" path="data/entity/character/" keep="true">
		<asset>parts/foot/creature.xml</asset>
		<asset>parts/hand/creature.xml</asset>
		<asset>animation/creature/navigation/stand.xml</asset>
		<asset>animation/creature/navigation/walk.xml</asset>
		<asset>animation/creature/navigation/jump.xml</asset>
		<asset>animation/creature/navigation/fall.xml</asset>
		<asset>animation/creature/navigation/land.xml</asset>
	</data>
	<assets type="creature" path="assets/entity/character/" keep="true">
		<asset>foot/creature.swf</asset>
		<asset>hand/creature.swf</asset>
	</assets>
	
	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2803</asset>
		<asset>2804</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2803</asset>
		<asset>2804</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundPink</asset>
	</assets>

	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>fs_concrete_01</asset>
		<asset>fs_concrete_02</asset>
		<asset>fs_concrete_03</asset>
		<asset>fs_concrete_04</asset>
		<asset>ls_concrete_01</asset>
		<asset>ls_concrete_02</asset>
		<asset>ls_bounce_01</asset>
		<asset>ls_bounce_02</asset>
		<asset>ls_bounce_03</asset>
	</assets>

	<!-- sound assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>twitch_game</asset>
		<asset>chase_game</asset>
	</assets>

