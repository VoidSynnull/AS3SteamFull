<bundle>
	<id>halloween2016</id>
	<name>Spooktacular Pack</name>

	<cardSets>
		<cardSet id="shared">
			<card>3547</card>
		</cardSet>
		<cardSet id="male">
			<card>3541</card>
			<card>3543</card>
			<card>3545</card>
		</cardSet>
		<cardSet id="female">
			<card>3542</card>
			<card>3544</card>
			<card>3546</card>
		</cardSet>
	</cardSets>
	
	<bundleNum>2</bundleNum>
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>-27</brightness>
	   			<contrast>0</contrast>
	   			<saturation>-27</saturation>
	   			<hue>-31</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/pumpkin.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>