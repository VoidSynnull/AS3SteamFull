<!-- set "Click through URL" field in CMS to campaign.xml to support handling of cards -->
<campaign>
  	<campaignId>ToyStory4HEVBB</campaignId> <!-- *** Must match campaign folder name -->
  	<version>2</version> <!-- version number that doesn't use steet paths -->
	<lockVideo>true</lockVideo> <!-- overrides locked video flag in hotspots.xml -->
 	
	<clickUrls>
		<clickUrl id="Default">https://ad.doubleclick.net/ddm/trackclk/N6344.1918853PLAYWIREMEDIA.COM/B23173087.254723447;dc_trk_aid=451155959;dc_trk_cid=103389622;dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=</clickUrl> <!-- Default clickURL as catch-all (use ad building clickURL)-->
		<clickUrl id="Video">https://ad.doubleclick.net/ddm/trackclk/N6344.1918853PLAYWIREMEDIA.COM/B23173087.254723747;dc_trk_aid=451155959;dc_trk_cid=103389622;dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=</clickUrl> <!-- for clicking on visit sponsor button on video end screen -->
		<clickUrl id="Card">https://ad.doubleclick.net/ddm/trackclk/N6344.1918853PLAYWIREMEDIA.COM/B23173087.254724002;dc_trk_aid=451155959;dc_trk_cid=103389622;dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=</clickUrl> <!-- for clicking on visit sponsor button on cards -->
	</clickUrls>
	
	<!-- third-party impressions by event name -->
	<impressionUrls>
		<impressions id="AdImpression"> <!-- For exterior ad building appeaing on main street -->
			<impression type="standard">https://ad.doubleclick.net/ddm/trackimp/N6344.1918853PLAYWIREMEDIA.COM/B23173087.254723447;dc_trk_aid=451155959;dc_trk_cid=103389622;ord=[timestamp];dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=?</impression>
		</impressions>
		<impressions id="VideoClicked"> <!-- For playing video -->
			<impression type="standard">https://ad.doubleclick.net/ddm/trackimp/N6344.1918853PLAYWIREMEDIA.COM/B23173087.254723747;dc_trk_aid=451155959;dc_trk_cid=103389622;ord=[timestamp];dc_lat=;dc_rdid=;tag_for_child_directed_treatment=;tfua=?</impression>
		</impressions>
		<impressions id="VideoReplayed"> <!-- For replaying video (usually same tag as for VideoClicked) -->
