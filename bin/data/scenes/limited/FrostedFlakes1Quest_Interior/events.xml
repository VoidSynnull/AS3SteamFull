<events>
	<group type="triggered" id="initScene">
		<event>
			<param1 id="action">setPart</param1>
			<param2 id="partID">facial</param2>
			<param3 id="partValue">pr_blader</param3>
		</event>
	</group>
	<group type="triggered" id="run">
		<event>
			<param1 id="action">lockInput</param1>
		</event>
		<event>
			<param1 id="action">moveToTarget</param1>
			<param2 id="npcID">player</param2>
			<param3 id="target">2100,640</param3>
		</event>
		<event>
			<param1 id="action">wait</param1>
			<param2 id="delay">2.5</param2>
		</event>
		<event>
			<param1 id="action">callFunction</param1>
			<param1 id="function">loadStartPopup</param1>
		</event>
	</group>	
</events>