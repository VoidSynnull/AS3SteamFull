<scene>
	<sceneType>default</sceneType>
	<cameraLimits>
		<left>0</left>
		<top>0</top>
		<right>960</right>
		<bottom>650</bottom>	
	</cameraLimits>
	<bounds>
		<left>0</left>
		<top>0</top>
		<right>960</right>
		<bottom>650</bottom>
	</bounds>
	<data>dialog.xml,doors.xml,npcs.xml,sounds.xml,hits.xml, levels.xml, traps.xml</data>
	<assets></assets>
	<layers>
		<layer>
			<asset>backdrop.swf</asset>
			<rate>1</rate>
		</layer>
		<layer>
			<asset>background.swf</asset>
			<rate>1</rate>
		</layer>
		<layer hit="true">
			<asset>interactive.swf</asset>
			<rate>1</rate>
		</layer>
		<layer>
			<asset>foreground.swf</asset>
			<rate>1</rate>
		</layer>
	</layers>
	<player>
		<defaultPosition>
			<x>200</x>
			<y>210</y>
		</defaultPosition>
		<defaultDirection>right</defaultDirection>
	</player>
</scene>