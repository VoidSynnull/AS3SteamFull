<manifest>
	<!-- campaign xml files -->
	<data type="campaign" path="data/limited/GetOutOfMyRoomMVU/" ext="xml">
		<asset>zipmanifest</asset>
		<asset>version</asset>
	</data>

	<!-- main street xml files and swfs -->
	<!-- from scene.xml in ad scene plus scene.xml -->
	<data type="game" path="data/limited/GetOutOfMyRoomMVU/" ext="xml">
		<asset>custom</asset>
		<asset>dialog</asset>
		<asset>hits</asset>
		<asset>hotspots</asset>
		<asset>npcs</asset>
		<asset>scene</asset>
		<asset>tracking</asset>
		<asset>Puzzle</asset>
	</data>
	<!-- from scene.xml in ad scene -->
	<assets type="game" path="assets/limited/GetOutOfMyRoomMVU/" ext="swf">
		<asset>background</asset>
		<asset>interactive</asset>
	</assets>
	
	<assets type="game" path="assets/limited/GetOutOfMyRoomMVU/" ext="swf">
		<asset>balloon1</asset>
		<asset>balloon2</asset>
		<asset>balloon3</asset>
		<asset>Puzzle</asset>
		<asset>StartPuzzle</asset>
		<asset>WinPuzzle</asset>
		<asset>LosePuzzle</asset>
		<asset>splatPopup</asset>
	</assets>

	<!-- music assets -->
	<assets type="sounds" path="sound/music/" ext="mp3" keep="true">
		<asset>GetHigh</asset>
	</assets>
	
	<!-- sound assets -->
	<assets type="sounds" path="sound/effects/" ext="mp3" keep="true">
		<asset>pop_05_louder</asset>
	</assets>

	<!-- card xml files, swfs, backs -->
	<data type="card" path="data/items/limited/" prefix="item" ext="xml">
		<asset>2900</asset>
	</data>
	<assets type="card" path="assets/items/limited/" prefix="item" ext="swf">
		<asset>2900</asset>
	</assets>
	<assets type="cardBack" path="assets/items/limited/backs/" ext="swf">
		<asset>AdBackgroundGrayBlue</asset>
	</assets>

	<!-- animation xml files -->
	<data type="animation" path="data/entity/character/animation/human/" ext="xml">
		<asset>throw</asset>
	</data>

	<!-- avatar parts -->
	<data type="mouth" path="data/entity/character/parts/mouth/" keep="true" ext="xml">
		<asset>p_wafflegirl</asset>
	</data>
	<assets type="mouth" path="assets/entity/character/mouth/" keep="true" ext="swf">
		<asset>p_wafflegirl</asset>
	</assets>
	<data type="facial" path="data/entity/character/parts/facial/" prefix="limited_" ext="xml">
		<asset>paintsplat</asset>
	</data>
	<assets type="facial" path="assets/entity/character/facial/" prefix="limited_" ext="swf">
		<asset>paintsplat</asset>
	</assets>
	<data type="hair" path="data/entity/character/parts/hair/" prefix="limited_" ext="xml">
		<asset>claire</asset>
	</data>
	<assets type="hair" path="assets/entity/character/hair/" prefix="limited_" ext="swf">
		<asset>claire</asset>
	</assets>
	<data type="shirt" path="data/entity/character/parts/shirt/" keep="true" ext="xml">
		<asset>sponsor_ramona</asset>
	</data>
	<assets type="shirt" path="assets/entity/character/shirt/" keep="true" ext="swf">
		<asset>sponsor_ramona</asset>
	</assets>
	<data type="pants" path="data/entity/character/parts/pants/" keep="true" ext="xml">
