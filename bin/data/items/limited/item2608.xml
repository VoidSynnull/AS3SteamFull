<item id="2608"> <!-- Change this card ID -->
  	<type>custom</type>
	<subType>general</subType>
	<name>Monster High Wish List</name> <!-- Change this card name -->
  	<campaignID>MonsterHighMomGirlWishlist</campaignID> <!-- Change this CMS name -->
	
  	<buttons>
  		<btn>
  			<index>1</index>
    		<label>PRINT THE LIST</label>
   			<actions>
				<action type="openPopup" tracking="Use Monster High Wish List"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="className">game.scenes.custom.WishlistPopup</param>
	  					<param id="swfPath">MonsterHighMomGirlWishlist/wishlist.swf</param> <!-- location of swf in assets/limited -->
						<param id="saveToggleButtons">true</param>
						<param id="trackToggleButtons">true</param>
					</parameters>
				</action>
			</actions>
  		</btn>
    	<btn>
    		<index>0</index>
      		<label>VISIT WEBSITE</label>
      		<actions>
				<action type="gotoUrl" tracking="Monster High Wish List"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="urlId">url1</param> <!-- NOTE :: This should get pulled from campaign.xml -->
					</parameters>
				</action>
	  		</actions>
    	</btn>
  	</buttons>
	
  	<textfields>
		<text id="limited">
			<value>FOR A LIMITED TIME ONLY</value>
		</text>
		<text id="subtitle">
			<value>Monster High</value> <!-- Change this subtitle -->
		</text>
		<text id="adtitle">
			<value>Wish List</value> <!-- Change this title -->
		</text>
		<text id="ad">
			<value>ADVERTISEMENT</value>
		</text>
		<text id="copyright">
			<value>Â© 2014 Mattel, Inc.</value> <!-- Change or delete this copyright -->
		</text>
  	</textfields>
	
  	<assets>
		<asset id="cardBack">
    		<assetPath>items/limited/backs/AdBackgroundGreen.swf</assetPath> <!-- Change this background swf -->
    	</asset>
		<asset id="cardContent" />
  	</assets>
    
</item>
