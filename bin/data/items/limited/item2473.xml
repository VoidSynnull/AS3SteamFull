<item id="2473"> <!-- Change this card ID -->
  	<type>custom</type>
	<subType>general</subType>
	<name>Crash the Turkey Card</name> <!-- Change this card name -->
  	<campaignID>PranksgivingQuest</campaignID> <!-- Change this CMS name -->
	
  	<buttons>
  		<btn>
  			<index>1</index>
    		<label>USE</label>
    		<actions>
				<action type="openPopup" tracking="Use Crash the Turkey Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="className">game.scenes.custom.CardAnimPower</param>
	  					<param id="swfPath">PranksgivingQuest/PranksgivingPower.swf</param> <!-- location of swf in assets/limited -->
	  					<param id="offsetX">100</param>
	  					<param id="offsetY">40</param>
	  					<param id="alignDirection">true</param>
					</parameters>
				</action>
			</actions>
  		</btn>
    	<btn>
    		<index>0</index>
      		<label>VISIT SITE</label>
      		<actions>
				<action type="gotoUrl" tracking="Crash the Turkey Card"> <!-- change tracking call -->
	  				<parameters>
	  					<param id="urlId">url1</param> <!-- NOTE :: This should get pulled from campaign.xml -->
					</parameters>
				</action>
	  		</actions>
    	</btn>
  	</buttons>
	
  	<textfields>
		<text id="limited">
			<value>FOR A LIMITED TIME ONLY</value>
		</text>
		<text id="subtitle">
			<value>Disney XD - Pranksgiving</value> <!-- Change this subtitle -->
		</text>
		<text id="subtitle">
			<value>Crash the Pranksgiving Turkey</value> <!-- Change this title -->
			<y>-182</y>
		</text>
		<text id="ad">
			<value>ADVERTISEMENT</value>
		</text>
  	</textfields>
	
  	<assets>
		<asset id="cardBack">
    		<assetPath>items/limited/backs/AdBackgroundGreen.swf</assetPath> <!-- Change this background swf -->
    	</asset>
		<asset id="cardContent" /> <!-- Will use content bin/assets/items/limited/item999.swf -->
  	</assets>
  	
</item>
